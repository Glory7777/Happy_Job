package kr.happyjob.study.scm.model;

public class ScmOrderModel {

	private String order_cd;
	private String pro_cd; 
	private String name;
	private String order_date; 
	private String order_hope_date; 
	private int order_price; 
	private String order_cnt;
	private String pro_nm;
	private int pro_sup_price;
	private int pro_unit_price;
	private int pro_stock;
	private String sup_cd;
	private String sup_nm;
	
	public String getOrder_cd() {
		return order_cd;
	}
	public void setOrder_cd(String order_cd) {
		this.order_cd = order_cd;
	}
	public String getPro_cd() {
		return pro_cd;
	}
	public void setPro_cd(String pro_cd) {
		this.pro_cd = pro_cd;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getOrder_date() {
		return order_date;
	}
	public void setOrder_date(String order_date) {
		this.order_date = order_date;
	}
	public String getOrder_hope_date() {
